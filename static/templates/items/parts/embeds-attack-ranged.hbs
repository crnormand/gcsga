<div class="items-section" data-types="attackRanged">
	<div class="items-header">
		<i class="fa-solid fa-checkmark"></i>
		<div class="item-header item-name">Ranged Weapon Usage</div>
		<div class="item-header item-skill-level">SL</div>
		<div class="item-header item-accuracy">Acc</div>
		<div class="item-header item-damage">Damage</div>
		<div class="item-header item-rof">RoF</div>
		<div class="item-header item-shots">Shots</div>
		<div class="item-header item-bulk">Bulk</div>
		<div class="item-header item-recoil">Recoil</div>
		<div class="item-header item-strength">ST</div>
		<div class="item-header item-controls"></div>
	</div>
	<ul class="action-list">
		{{#each attacksRanged}} {{> "gurps.embeds-action" action=this}} {{/each}}
	</ul>
</div>
